#include <stdio.h>
int main () 
{
    int num,i;
    int binary[32];
    printf ("ENTER A NUMBER\n");
    scanf ("%d",&num);
    if (num==0)
    {
        printf("0\n");
    }
    i=0;
    while (num>0)
    {
        binary[i]=num%2;
        num=num/2;
        i++;
    }
    for (int j=i -1; j>=0;j--)
    {
        printf("%d",binary[j]);
    }
    printf("\n");
    return 0;
}
